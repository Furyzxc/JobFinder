{
	"trailingComma": "es5",
	"useTabs": true,
	"tabWidth": 2,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/assets/(.*)$",
		"^@/app/(.*)$",
		"^@/shared/(.*)$",
		"^@/components/(.*)$",
		"^(.*)/model/(.*)$",
		"^(.*)/lib/(.*)$",
		"^(.*)/entities/(.*)$",
		"^(.*)/features/(.*)$",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": false,
	"importOrderSortSpecifiers": true
}
